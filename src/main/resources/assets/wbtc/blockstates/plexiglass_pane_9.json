{
    "multipart": [
        {   "apply": { "model": "wbtc:plexiglass_pane_9_post" }},
        {   "when": { "north": true },
            "apply": { "model": "wbtc:plexiglass_pane_9_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "wbtc:plexiglass_pane_9_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "wbtc:plexiglass_pane_9_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "wbtc:plexiglass_pane_9_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "wbtc:plexiglass_pane_9_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "wbtc:plexiglass_pane_9_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "wbtc:plexiglass_pane_9_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "wbtc:plexiglass_pane_9_noside", "y": 270 }
        }
    ]
}
